<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane stylesheets="@../../css/main.css" xmlns="http://javafx.com/javafx/8.0.141"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="fxcontrollers.NodeViewController">
    <children>
        <VBox>
            <TableView fx:id="nodeTableView" >
                <columns>
                    <TableColumn fx:id="idColumn" text="ID"/>
                    <TableColumn fx:id="aggrWinColumn" text="AggrWin"/>
                    <TableColumn fx:id="nameColumn" text="Name"/>
                    <TableColumn fx:id="noteColumn" text="Note"/>
                    <TableColumn fx:id="swColumn" text="SW"/>
                    <TableColumn fx:id="typeColumn" text="Type"/>
                </columns>

                <placeholder>
                    <Label text="No results" />
                </placeholder>
            </TableView>
        </VBox>
    </children>
</StackPane>
